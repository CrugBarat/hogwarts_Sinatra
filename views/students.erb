<div class="content-container">
  <div class="student-deets">
    <%for student in @students %>
    <img src="<%=student.house_logo%>">
    <p><%=student.full_name() %></p>
    <p>Age: <%=student.age%></p>
    <p><%=student.house_name %></p>
    <br>
    <%end%>
  </div>
</div>


<div class="button-container">
  <a href="/home/students/create">
    <button class="button" type="button" name="button">Create New Student</button>
  </a>
</div>
